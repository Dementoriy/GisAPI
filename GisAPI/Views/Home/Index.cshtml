@model Addresses
@{
    ViewData["Title"] = "Home Page";
}

<form asp-controller="home" asp-action="index" class="text-center" enctype="multipart/form-data" method="post" style="padding-top: 10px">
    <label>Выберите excel файл: </label>
    <input type="file" asp-for="Address" accept=".xlsx" onchange="this.form.submit()">
</form>

<div id="container" style="margin: 0; width: 100%; height: 1000px; overflow-y: hidden; padding-top: 10px"></div>

<script src="https://mapgl.2gis.com/api/js/v1"></script>

<script>
    const map = new mapgl.Map('container', {
    key: 'b4cf0277-d6a6-4d6d-8f5f-a246c6f6ac64',
    center: [49.66, 58.60],
    zoom: 13,
    });
</script>

